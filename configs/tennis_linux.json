{
  "env_name": "Tennis_Linux",
  "env_path": [".", "p3_collab-compet", "Tennis_Linux", "Tennis.x86_64"],
  "monitor_dir": [ "monitor" ],
  "checkpoint_dir": [ "checkpoints" ],
  "device": "cuda:0",
